{
  "name": "livepeer-alerts-frontend",
  "version": "0.1.0",
  "description": "Provide pro-active alert notifications that will help LPT token holders to be updated and understand how the transcoders they are delegating the tokens to are performing in near real time.",
  "author": "Protofire @protofire",
  "license": "MIT",
  "bugs": "https://github.com/protofire/livepeer-alerts-frontend/issues",
  "keywords": [
    "ethereum",
    "blockchain",
    "livepeer"
  ],
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bignumber.js": "^8.0.2",
    "bootstrap": "^4.3.1",
    "emoji-mart": "^2.9.2",
    "history": "4.7.2",
    "logdown": "^3.2.7",
    "moment": "2.22.2",
    "npm-run-all": "4.1.3",
    "polished": "^3.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.6.3",
    "react-ga": "^2.5.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-toastify": "^4.5.1",
    "react-tooltip": "^3.10.0",
    "sanitize.css": "^11.0.0",
    "styled-components": "^4.3.2",
    "validator": "^10.10.0",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "babel-eslint": "^9.0.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "1.3.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint . src/**/*.js",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "eslintConfig": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/assets/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run lint"
    }
  }
}
